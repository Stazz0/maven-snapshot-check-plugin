<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:l="/lib/layout" xmlns:st="jelly:stapler">
    <l:layout title="Maven SNAPSHOT Check">
        <l:side-panel>
            <st:include page="sidepanel.jelly" it="${it.run}" optional="true" />
        </l:side-panel>
        <l:main-panel>
            <h3>
                Is it checked?
            </h3>
            <h3>
                ${it.message}
            </h3>
        </l:main-panel>
    </l:layout>
</j:jelly>